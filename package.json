{
  "name": "@avi/vue-components",
  "version": "1.0.1",
  "description": "Reusable vue components",
  "author": {
    "name": "Avi Mehenwal",
    "email": "avi.mehanwal@gmail.com"
  },
  "scripts": {
    "serve": "yarn --cwd ./Apps/default/ serve",
    "build:app:umd": "yarn run clean && yarn --cwd ./Apps/default/ vue-cli-service build --target lib --inline-vue --report && yarn run ls",
    "build:app": "yarn run clean && yarn --cwd ./Apps/default/ build --report && yarn run ls",
    "tbs": "yarn jest && yarn build && yarn serve",
    "clean": "rm -rfv ./Apps/default/dist/",
    "ls": "tree ./Apps/default/dist/",
    "pub": "npm publish --dry-run && yarn --verbose publish",
    "build": "yarn run build:umd & yarn run build:es & yarn run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/my-component.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/my-component.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/my-component.min.js"
  },
  "files": [
    "src/*"
  ],
  "main": "dist/my-component.umd.js",
  "module": "dist/my-component.esm.js",
  "unpkg": "dist/my-component.min.js",
  "browser": {
    "./sfc": "src/my-component.vue"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@vue/compiler-sfc": "^3.0.0-rc.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/animejs": "^3.1.1",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-e2e-cypress": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-pwa": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "animejs": "^3.2.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "faker": "^4.1.0",
    "jest": "^26.1.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.12.0",
    "register-service-worker": "^1.7.1",
    "rollup": "^2.23.0",
    "rollup-plugin-vue": "^6.0.0-beta.9",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.5",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-google-charts": "^0.3.3",
    "vue-jest": "^3.0.6",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.3.4",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.3.6",
    "vuetify-loader": "^1.3.0",
    "vuex": "^3.5.1",
    "w3-css": "^4.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/avimehenwal/vue-components/issues"
  },
  "homepage": "https://github.com/avimehenwal/vue-components#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:avimehenwal/vue-components.git"
  },
  "keywords": [
    "vuejs",
    "vue",
    "component",
    "vue2",
    "vuejs2"
  ]
}
